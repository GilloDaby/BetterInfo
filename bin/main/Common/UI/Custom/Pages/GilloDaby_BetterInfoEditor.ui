$C = "../Common.ui";

Group #EditorRoot {
  Anchor: (Full: 0);
  LayoutMode: Middle;
  Background: #0a0c10(0.82);

  Group #Panel {
    Anchor: (Width: 900, Height: 840);
    LayoutMode: Top;
    Padding: (Left: 22, Right: 22, Top: 18, Bottom: 18);
    Background: (TexturePath: "../Common/Popup.png", Border: 14);

    Group #ContentScroll {
      Anchor: (Width: 856, Height: 620);
      LayoutMode: TopScrolling;
      Padding: (Left: 6, Right: 6, Top: 6, Bottom: 6);

      Group #Header {
        Anchor: (Width: 832);
        LayoutMode: Top;
        Padding: (Left: 6, Right: 6, Top: 6, Bottom: 12);

        Label #EditorTitle {
          @Text = "Better Info Editor";
          Anchor: (Width: 820, Height: 34);
          Style: (FontSize: 22, RenderBold: true, TextColor: #f6f8ff, HorizontalAlignment: Center, VerticalAlignment: Center);
        }

        Label #PlaceholderHint {
          @Text = "Use [#rrggbb] at the start of a segment to color it";
          Anchor: (Width: 820, Height: 20);
          Style: (FontSize: 13, TextColor: #c8d2e3, HorizontalAlignment: Center, VerticalAlignment: Center);
        }

        Label #LimitHint {
          @Text = "Max info lines shown per page: 50";
          Anchor: (Width: 820, Height: 20);
          Style: (FontSize: 12, TextColor: #8ca0bf, HorizontalAlignment: Center, VerticalAlignment: Center);
        }

        Label #ColorHint {
          @Text = "Each page supports up to 50 color-aware lines";
          Anchor: (Width: 820, Height: 20);
          Style: (FontSize: 12, TextColor: #8ca0bf, HorizontalAlignment: Center, VerticalAlignment: Center);
        }

        Group #TitleRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 60);
          Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);

          Label {
            Text: "Page title";
            Anchor: (Width: 200, Height: 40);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #TitleInput {
            FlexWeight: 1;
            PlaceholderText: "New page title";
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #TitleColorHex {
            Anchor: (Width: 140, Height: 38);
            PlaceholderText: "#ffffff";
          }
        }

        Group #HeaderHintRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 56);
          Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);

          Label {
            Text: "Header hint";
            Anchor: (Width: 200, Height: 34);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #HeaderHintInput {
            FlexWeight: 1;
            PlaceholderText: "Share helpful guidance for players.";
          }
        }

        Group #CommandRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 56);
          Padding: (Left: 12, Right: 12, Top: 4, Bottom: 8);

          Label {
            Text: "Command code";
            Anchor: (Width: 200, Height: 34);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #CommandCodeInput {
            Anchor: (Width: 220, Height: 38);
            PlaceholderText: "tos";
          }

          Group { Anchor: (Width: 12); }

          Label #CommandHint {
            @Text = "Players can use /<code>";
            FlexWeight: 1;
            Style: (FontSize: 12, TextColor: #9fc5ff, VerticalAlignment: Center);
          }
        }

        Group #ButtonRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 54);
          Padding: (Left: 12, Right: 12, Top: 2, Bottom: 2);

          Label {
            Text: "Button text";
            Anchor: (Width: 200, Height: 34);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #ButtonTextInput {
            FlexWeight: 1;
            PlaceholderText: "Thanks for the info, let's go!";
          }
        }

        Group #FooterRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 54);
          Padding: (Left: 12, Right: 12, Top: 2, Bottom: 2);

          Label {
            Text: "Footer text";
            Anchor: (Width: 200, Height: 34);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #FooterTextInput {
            FlexWeight: 1;
            PlaceholderText: "You can reopen this page anytime with /<code>";
          }
        }

        Group #NextPageRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 54);
          Padding: (Left: 12, Right: 12, Top: 2, Bottom: 6);

          Label {
            Text: "Next page";
            Anchor: (Width: 200, Height: 34);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 10); }

          $C.@TextField #NextPageInput {
            Anchor: (Width: 120, Height: 38);
            PlaceholderText: "2";
          }

          Group { Anchor: (Width: 12); }

          Label #NextPageHint {
            @Text = "Leave blank to confirm/close";
            FlexWeight: 1;
            Style: (FontSize: 12, TextColor: #9fc5ff, VerticalAlignment: Center);
          }
        }

        Group #PageRow {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 40);
          Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);

          Label {
            Text: "Pages";
            Anchor: (Width: 90, Height: 30);
            Style: (FontSize: 14, TextColor: #c8d2e3, VerticalAlignment: Center);
          }

          Group { Anchor: (Width: 8); }

          $C.@SmallSecondaryTextButton #Page1Button { @Text = "Page 1"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page2Button { @Text = "Page 2"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page3Button { @Text = "Page 3"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page4Button { @Text = "Page 4"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page5Button { @Text = "Page 5"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page6Button { @Text = "Page 6"; Anchor: (Width: 95, Height: 30); }
        }

        Group #PageRow2 {
          LayoutMode: Left;
          Anchor: (Width: 820, Height: 40);
          Padding: (Left: 12, Right: 12, Top: 4, Bottom: 4);

          Group { Anchor: (Width: 90); }

          $C.@SmallSecondaryTextButton #Page7Button { @Text = "Page 7"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page8Button { @Text = "Page 8"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page9Button { @Text = "Page 9"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page10Button { @Text = "Page 10"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page11Button { @Text = "Page 11"; Anchor: (Width: 95, Height: 30); }
          Group { Anchor: (Width: 6); }
          $C.@SmallSecondaryTextButton #Page12Button { @Text = "Page 12"; Anchor: (Width: 95, Height: 30); }

          Group { FlexWeight: 1; }

          Label #ActivePageLabel {
            @Text = "Page active: 1";
            Anchor: (Width: 160, Height: 30);
            Style: (FontSize: 13, TextColor: #9fc5ff, HorizontalAlignment: End, VerticalAlignment: Center);
          }
        }
      }

      Group { Anchor: (Height: 12); }

      Group #ScrollArea {
